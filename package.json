    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "gh-pages": "^6.3.0",
    "husky": "^9.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4",
    "typescript": "^5"
