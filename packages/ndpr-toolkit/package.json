{
  "name": "@tantainnovative/ndpr-toolkit",
  "version": "1.0.7",
  "description": "A comprehensive toolkit for implementing NDPR-compliant features in web applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./unstyled": {
      "types": "./dist/unstyled.d.ts",
      "import": "./dist/unstyled.mjs",
      "require": "./dist/unstyled.js"
    },
    "./styles": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup && pnpm build:types",
    "build:types": "tsc -p tsconfig.lib.json",
    "dev": "tsup --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ndpr",
    "data-protection",
    "privacy",
    "compliance",
    "nigeria",
    "consent",
    "dpia",
    "data-subject-rights",
    "breach-notification"
  ],
  "author": "Tanta Innovative",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tantainnovative/ndpr-toolkit"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tslib": "^2.4.0"
  }
}
